{
  "name": "tgsplat-enterprise",
  "version": "2.0.0",
  "description": "Enterprise-grade Telegram mini app for viewing Gaussian splats with modular architecture",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "node server.js",
    "build": "webpack --mode=production",
    "build:dev": "webpack --mode=development",
    "watch": "webpack --mode=development --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "lint": "eslint src/ --ext .js",
    "lint:fix": "eslint src/ --ext .js --fix",
    "format": "prettier --write \"src/**/*.{js,css}\"",
    "format:check": "prettier --check \"src/**/*.{js,css}\"",
    "analyze": "webpack-bundle-analyzer dist/stats.json",
    "setup-bot": "node setup-bot.js",
    "check-webhook": "node setup-bot.js check",
    "deploy": "node deploy-railway.js",
    "deploy-complete": "node deploy-complete.js",
    "railway:login": "railway login",
    "railway:logs": "railway logs",
    "railway:domain": "railway domain",
    "docs:generate": "jsdoc -c jsdoc.conf.json",
    "security:audit": "npm audit",
    "security:fix": "npm audit fix",
    "performance:profile": "node --prof src/performance/profiler.js",
    "clean": "rimraf dist/ coverage/ docs/",
    "precommit": "lint-staged",
    "prepare": "husky install"
  },
  "keywords": [
    "telegram",
    "mini-app",
    "gaussian-splats",
    "three.js",
    "3d",
    "enterprise",
    "modular-architecture",
    "webgl",
    "performance-optimized"
  ],
  "author": "TGSplat Enterprise Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "compression": "^1.7.4",
    "helmet": "^7.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "winston": "^3.11.0",
    "rate-limiter-flexible": "^3.0.8"
  },
  "devDependencies": {
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-bundle-analyzer": "^4.9.1",
    "webpack-dev-middleware": "^6.1.1",
    "babel-loader": "^9.1.3",
    "@babel/core": "^7.23.3",
    "@babel/preset-env": "^7.23.3",
    "css-loader": "^6.8.1",
    "mini-css-extract-plugin": "^2.7.6",
    "terser-webpack-plugin": "^5.3.9",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "@testing-library/jest-dom": "^6.1.4",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jest": "^27.6.0",
    "prettier": "^3.1.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.1.0",
    "rimraf": "^5.0.5",
    "jsdoc": "^4.0.2",
    "jsdoc-to-markdown": "^8.0.0",
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": ["<rootDir>/tests/setup.js"],
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/*.test.js",
      "!src/workers/**",
      "!**/node_modules/**"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "testMatch": [
      "<rootDir>/tests/**/*.test.js"
    ],
    "moduleNameMapping": {
      "^@/(.*)$": "<rootDir>/src/$1"
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "eslint --fix",
      "prettier --write"
    ],
    "src/**/*.css": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test:ci"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/tgsplat-enterprise.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/tgsplat-enterprise/issues"
  },
  "homepage": "https://github.com/your-org/tgsplat-enterprise#readme"
}